<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
        <attr name="left_top_radius" format="dimension"/>
        <attr name="right_top_radius" format="dimension"/>
        <attr name="left_bottom_radius" format="dimension"/>
        <attr name="right_bottom_radius" format="dimension"/>
        <attr name="radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SearchEditText">
        <attr name="show_search_drawable" format="boolean"/>
        <attr name="search_drawable" format="reference"/>
        <attr name="clear_drawable" format="reference"/>
    </declare-styleable>
</resources>